# 20
.intel_syntax noprefix
mov rax, 0
mov r12, rsi
loop:
        cmp rsi, 0
        je done
        mov ebx, dword ptr [rdi]
        add rax, rbx
        add rdi, 4
        dec rsi
        jmp loop:
done:
        div r12

gcc -nostdlib -o solve.elf flag20.s
./solve.elf
objdump -M intel -d solve.elf
objcopy --dump-section .text=solve.bin solve.elf
cat solve.bin | /challenge/embryoasm_level20

# 22
.global _start
.intel_syntax noprefix

str_lower:
    push rbp
    mov rbp, rsp
    
    mov r12, 0
    cmp rdi, 0
    je done

    checker:
    mov bl, BYTE PTR [rdi]
    cmp bl, 0
    je done
    jmp loopy
    
    loopy:
    mov bl, BYTE PTR [rdi]
    cmp bl, 90
    jle act
    jmp incre

    act:
    mov r10, rdi
    mov dil, bl
    mov rcx, 0x403000
    call rcx
    mov rdi, r10
    mov BYTE PTR [rdi], al
    inc r12
    jmp incre

    incre:
    inc rdi
    jmp checker

    done:
    mov rcx, r12
    mov rax, rcx
    pop rbp
    ret

gcc -nostdlib -o solve.elf flag22.s
./solve.elf
objdump -M intel -d solve.elf
objcopy --dump-section .text=solve.bin solve.elf
cat solve.bin | /challenge/embryoasm_level22

pwn.college{AKT12FabZAUL5uN_Y8hRsZEug2j.QX1EjMscTOxUzW}

# 23
.intel_syntax noprefix

push rbp
mov rbp, rsp
mov rcx, 0
mov rax, 0
jmp j1

j1: 
cmp rcx, rsi
jge j2

xor rdx, rdx
mov rdx, rdi
add edx, ecx

mov al, BYTE PTR [rdx]
mov rdx, rsp
sub rdxm rax
mov r12b, BYTE PTR [rdx]
inc r12b
mov BYTE PTR [rdx], r12b
inc rcx
jmp j1

j2:
mov rbx, 0
mov rcx, 0
mov rax, 0

jmp j3

j3:
cmp rbx, 0xff
jg done

mov rdx, 0
mov rdx, rbp
sub rdx, rbx
cmp BYTE PTR [rdx], cl
jge j3

mov cl, BYTE PTR [rdx]
mov al, bl
add bl, 1
jmp j3

done:
mov rsp, rbp
pop rbp
ret

gcc -nostdlib -o solve.elf flag23.s
./solve.elf
objdump -M intel -d solve.elf
objcopy --dump-section .text=solve.bin solve.elf
cat solve.bin | /challenge/embryoasm_level23



# python to assembly
import pwn

pwn.context.arch = "amd64"
pwn.context.encoding = "latin-1"
pwn.context.log_level = "INFO"
pwn.warnings.simplefilter("ignore")
assmebly = """

mov rdi, 0x1337

mov rax, SYS_exit
syscall
"""

with pwn.process(f"/challenge/{pwn.os.getenv('HOSTNAME')}") as target:
    pwn.info(target.readrepeat(1))

    targ.send(pwn.asm(assmembly))
    pwn.info(target.readrepeat(1))
#print(pwn.asm(assembly))